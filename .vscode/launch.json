    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "SuttApplication",
            "request": "launch",
            "mainClass": "com.codinglynx.sutt.SuttApplication",
            "projectName": "sutt"
        },
        {
            "name": "angular app",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceRoot}/src/app",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script", "start"
            ],
        }
    ]
}